<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/cardViewFragment">
    <fragment
        android:id="@+id/cardViewFragment"
        android:name="com.uottawahack.chefswipe.CardViewFragment"
        android:label="fragment_card_view"
        tools:layout="@layout/fragment_card_view" >
        <action
            android:id="@+id/action_cardViewFragment2_to_savedRecipes"
            app:destination="@id/savedRecipes"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/savedRecipes"
        android:name="com.uottawahack.chefswipe.SavedRecipeFragment"
        android:label="fragment_saved_recipe"
        tools:layout="@layout/fragment_saved_recipe" />
</navigation>